      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@elasticontent/figma-react-design-system": "^3.0.22",
    "@svgr/cli": "^5.4.0",
